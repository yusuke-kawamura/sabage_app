<% if model.errors.present? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      入力に誤りがあります <%= pluralize(model.errors.count, "エラー") %>.
    </div>
    <ul>
    <% model.errors.full_messages.each do |msg| %>
      <li><%=msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>